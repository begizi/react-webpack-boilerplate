{
  "name": "react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "A simple react, webpack, stylus, and hot reloading boilerplate",
  "main": "src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.js",
    "build": "NODE_ENV=production webpack -p --config webpack.dist.config.js",
    "test": "NODE_PATH=./node_modules:./src jest .test.js"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-jest": "^6.0.1",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "jest-cli": "^0.8.2",
    "node-libs-browser": "^0.5.3",
    "react-hot-loader": "^1.3.0",
    "require-dir": "^0.3.0",
    "style-loader": "^0.13.0",
    "stylus-loader": "^1.4.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "history": "^1.17.0",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-router": "^1.0.3"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/preprocessor.js",
    "testDirectoryName": "src",
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react"
    ]
  }
}
