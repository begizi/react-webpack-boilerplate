    "css-loader": "^0.23.0",
    "eslint": "^1.9.0",
    "eslint-plugin-react": "^3.8.0",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "gulp": "^3.9.0",
    "gulp-eslint": "^1.1.0",
    "gulp-html-replace": "^1.5.4",
    "gulp-jest": "^0.4.0",
